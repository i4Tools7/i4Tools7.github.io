 function switchpage(pageid) {
 	    switch (pageid) {
            case 1:
                tab1.className = 'tabsel';
                tab2.className = 'tab';
                tab3.className = 'tab';
                tab4.className = 'tab';
                tab5.className = 'tab';
                tab6.className = 'tab';
                tab7.className = 'tab';             
                divpage1.style.display = '' ;
                divpage2.style.display = 'none' ;
                divpage3.style.display = 'none' ;
                divpage4.style.display = 'none' ;
                divpage5.style.display = 'none' ;
                divpage6.style.display = 'none' ;
                divpage7.style.display = 'none' ;
                setCookie("tabsel","1",null);
                document.getElementById("IFrame2").src='about:blank';
                document.getElementById("IFrame3").src='about:blank';
                document.getElementById("IFrame4").src='about:blank';
                document.getElementById("IFrame5").src='about:blank';                
                document.getElementById("IFrame6").src='about:blank';
                document.getElementById("IFrame7").src='about:blank';
            break;
            case 2:
                tab1.className = 'tab';
                tab2.className = 'tabsel';
                tab3.className = 'tab';
                tab4.className = 'tab';
                tab5.className = 'tab';
                tab6.className = 'tab';
                tab7.className = 'tab';
                divpage1.style.display = 'none' ;
                divpage2.style.display = '' ;
                divpage3.style.display = 'none' ;
                divpage4.style.display = 'none' ;
                divpage5.style.display = 'none' ;
                divpage6.style.display = 'none' ;               
                divpage7.style.display = 'none' ;   
                setCookie("tabsel","2",null);
                document.getElementById("IFrame2").src='/Kiosk/ccs/jsp/listServiceType.do';
                document.getElementById("IFrame3").src='about:blank';
                document.getElementById("IFrame4").src='about:blank';
                document.getElementById("IFrame5").src='about:blank';                
                document.getElementById("IFrame6").src='about:blank';
                document.getElementById("IFrame7").src='about:blank';
            break;
            case 3:
                tab1.className = 'tab';
                tab2.className = 'tab';
                tab3.className = 'tabsel';
                tab4.className = 'tab';
                tab5.className = 'tab';
                tab6.className = 'tab';
                tab7.className = 'tab';
                divpage1.style.display = 'none' ;
                divpage2.style.display = 'none' ;
                divpage3.style.display = '' ;
                divpage4.style.display = 'none' ;
                divpage5.style.display = 'none' ;
                divpage6.style.display = 'none' ;
                divpage7.style.display = 'none' ;
                setCookie("tabsel","3",null);           
                document.getElementById("IFrame2").src='about:blank';
                document.getElementById("IFrame3").src='/Kiosk/ccs/jsp/list_current_ticketlog_detail.jsp';
                document.getElementById("IFrame4").src='about:blank';
                document.getElementById("IFrame5").src='about:blank';                
                document.getElementById("IFrame6").src='about:blank';
                document.getElementById("IFrame7").src='about:blank';
            break;
            case 4:
                tab1.className = 'tab';
                tab2.className = 'tab';
                tab3.className = 'tab';
                tab4.className = 'tabsel';
                tab5.className = 'tab';
                tab6.className = 'tab';
                tab7.className = 'tab';
                divpage1.style.display = 'none' ;
                divpage2.style.display = 'none' ;
                divpage3.style.display = 'none' ;
                divpage4.style.display = '' ;
                divpage5.style.display = 'none' ;
                divpage6.style.display = 'none' ;
                divpage7.style.display = 'none' ;
                setCookie("tabsel","4",null);
                document.getElementById("IFrame2").src='about:blank';
                document.getElementById("IFrame3").src='about:blank';
                document.getElementById("IFrame4").src='/Kiosk/ccs/jsp/list_calculate_waiting_ticket.jsp';
                document.getElementById("IFrame5").src='about:blank';                
                document.getElementById("IFrame6").src='about:blank';
                document.getElementById("IFrame7").src='about:blank';                
            break;
            case 5:
                tab1.className = 'tab';
                tab2.className = 'tab';
                tab3.className = 'tab';
                tab4.className = 'tab';
                tab5.className = 'tabsel';
                tab6.className = 'tab';
                tab7.className = 'tab';
                divpage1.style.display = 'none' ;
                divpage2.style.display = 'none' ;
                divpage3.style.display = 'none' ;
                divpage4.style.display = 'none' ;
                divpage5.style.display = '' ;
                divpage6.style.display = 'none' ;
                divpage7.style.display = 'none' ;
                setCookie("tabsel","5",null);
                document.getElementById("IFrame2").src='about:blank';
