                document.getElementById("IFrame6").src='/Kiosk/ccs/jsp/list_history_ticketlog_detail.jsp';
                document.getElementById("IFrame7").src='about:blank';                
            break;
            case 7:
                tab1.className = 'tab';
                tab2.className = 'tab';
                tab3.className = 'tab';
                tab4.className = 'tab';
                tab5.className = 'tab';
                tab6.className = 'tab';
                tab7.className = 'tabsel';
                divpage1.style.display = 'none' ;
                divpage2.style.display = 'none' ;
                divpage3.style.display = 'none' ;
                divpage4.style.display = 'none' ;
                divpage5.style.display = 'none' ;
